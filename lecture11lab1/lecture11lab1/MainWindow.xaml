<Window x:Class="lecture11lab1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lecture11lab1"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Background="#FF4D9F68">
    <Grid>
        <Button x:Name="fetchButton" Content="Fetch Phone #" HorizontalAlignment="Left" Margin="36,102,0,0" VerticalAlignment="Top" Width="120" Click="Button_Click"/>
        <TextBox x:Name="entryBox" HorizontalAlignment="Left" Height="23" Margin="36,61,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextAlignment="Center"/>
        <Label Content="Please Enter Name:" HorizontalAlignment="Left" Margin="36,30,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBlock x:Name="outputText" HorizontalAlignment="Left" Margin="36,148,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="59" Width="120" TextAlignment="Center"/>

    </Grid>
</Window>
